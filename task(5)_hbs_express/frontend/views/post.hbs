
{{>nav}}
<form action="/addUserPost" method="post" >
    <div class="form-floating m-2 mb-3 fs-6">
        <input type="text" class="form-control" name="name" placeholder="enter name" required>
        <label for="floatingInput">name</label>
    </div>
    <div class="form-floating m-2 mb-3 fs-6">
        <input type="text" class="form-control" name="age" placeholder="enter age" required>
        <label for="floatingInput ">age</label>
    </div>
    <div class="form-floating m-2 mb-3 fs-6">
        <input type="email" class="form-control" name="email" placeholder="enter email" required>
        <label for="floatingInput ">email</label>
    </div>
    <div class="d-flex justify-content-between">
        <div class="form-floating m-2 mb-3 fs-6  "   style="min-width: 49%;">
            <input type="text" class="form-control" name="addressType" placeholder="enter addressType" required>
            <label for="floatingInput">addres.addressType</label>
        </div>
        <div class="form-floating m-2 mb-3 fs-6"  style="min-width: 49%;">
            <input type="text" class="form-control" name="addressDetails" placeholder="enter addressDetails" required>
            <label for="floatingInput ">addres.addressDetails</label>
        </div>
    </div>
    <div class="d-flex justify-content-center mt-5">
        <button  class="btn btn-primary mx-3 fs-5 mt-2">Add User</button>
    </div>

</form>
{{>footer}}