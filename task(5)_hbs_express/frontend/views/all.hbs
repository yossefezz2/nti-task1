{{>nav}}
<table class="table table-bordered ">
    <thead>
        <tr>
            <th>ID</th>
            <th>name</th>
            <th>email</th>
            <th>age</th>
            <th>address type</th>
            <th>address details</th>
            <th>action</th>

        </tr>
    </thead>
    <tbody>
        {{#each users}}
        <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{email}}</td>
            <td>{{age}}</td>
            {{#each address}}
            <td>{{addressType}}</td>
            <td>{{addressDetails}}</td>
            {{/each}}
            <td>
                <a href="/single/{{id}}" class="btn btn-secondary mx-2">Show</a>
                <a href="/edit/{{id}}" class="btn btn-warning mx-2">Edit</a>
                <a href="/del/{{id}}" class="btn btn-danger mx-2">Delete</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
{{>footer}}