{{>nav}}
<form method="post" action="/editUserPost/{{allUsers.id}}">
    <div class="form-floating m-2 mb-3 fs-6">
        <input type="text" class="form-control" name="title" placeholder="enter title" value="{{allUsers.title}}" >
        <label for="floatingInput">name</label>
    </div>
    <div class="form-floating m-2 mb-3 fs-6">
        <input type="text" class="form-control" name="age" placeholder="enter age" value="{{allUsers.age}}" >
        <label for="floatingInput ">age</label>
    </div>
    <div class="form-floating m-2 mb-3 fs-6">
        <input type="email" class="form-control" name="email" placeholder="enter email" value="{{allUsers.email}}" >
        <label for="floatingInput ">email</label>
    </div>
    </div>
        <div class=" m-2 mb-3 fs-6">
        <label for="status"> status</label>
        <input type="checkbox" name="status"    {{#if allUsers.status }} checked {{/if}} >
    </div>
    <div class="d-flex justify-content-center mt-5">
        <button  class="btn btn-warning mx-3 fs-5 mt-2">edit User</button>
    </div>

</form>
{{>footer}}